<Window x:Class="WpfApp1.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:WpfApp1"
        mc:Ignorable="d"
        Title="MainWindow" Height="500" Width="600">
    <Grid Width="600" Height="500">
        <Grid Width="400" Margin="1,40,199,50">

            <Image x:Name="RoadMap"/>
            <Image x:Name="StopSign" Width="20"/>
            <Label x:Name="mouseMover" Content="" HorizontalAlignment="Left" Margin="0,-31,0,0" VerticalAlignment="Top" Width="160"/>
        </Grid>
        <ComboBox SelectionChanged="ElementsCMB_SelectionChanged" Name="ElementsCMB" Text="Добавить объект"  HorizontalAlignment="Left" Margin="385,0,0,0" VerticalAlignment="Top" Width="205"/>
    </Grid>
</Window>
